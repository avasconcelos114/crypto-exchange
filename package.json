{
  "name": "crypto-exchange",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@emotion/react": "^11.9.0",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.26.1",
    "axios-retry": "^3.2.4",
    "fflate": "^0.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modal": "^3.14.4",
    "react-query": "^3.35.0",
    "react-redux": "^8.0.0",
    "react-router-dom": "^5.3.1",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "plugins": [
      "react",
      "prettier"
    ],
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": 0
    },
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "~api/*",
              "./src/api/*"
            ],
            [
              "~components/*",
              "./src/components/*"
            ],
            [
              "~lib/*",
              "./src/lib/*"
            ],
            [
              "~resources/*",
              "./src/resources/*"
            ],
            [
              "~store/*",
              "./src/store/*"
            ]
          ],
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.9.2",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "craco-alias": "^3.0.1",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "http-proxy-middleware": "^2.0.6",
    "prettier": "^2.5.1"
  }
}
